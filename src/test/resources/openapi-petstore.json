{
  "openapi": "3.0.0",
  "info": { "title": "Petstore", "version": "1.0" },
  "servers": [{ "url": "https://petstore.example.com/v1" }],
  "paths": {
    "/pets": {
      "get": {
        "summary": "List pets",
        "parameters": [{ "name": "limit", "in": "query" }]
      },
      "post": {
        "summary": "Create pet",
        "requestBody": { "content": { "application/json": {} } }
      }
    },
    "/pets/{id}": {
      "get": {
        "summary": "Get pet",
        "parameters": [{ "name": "id", "in": "path" }]
      }
    }
  }
}
